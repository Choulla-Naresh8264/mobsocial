    
<div class="customer-friends-container" ng-controller="FriendsController" ng-init="GetCustomerFriends()">
    <div class="customer-friends-friend-block" ng-repeat="friend in CustomerFriends">
        <a href="{{friend.ProfileUrl}}">
            <img ng-src="{{friend.ProfileThumbnailUrl}}" alt="" title="{{friend.CustomerDisplayName}}" />
        </a>
    </div>
   
</div>
